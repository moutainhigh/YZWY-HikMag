<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/li_homemenu_item_frgmthome"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/press_home_menu_style"
        android:gravity="center"
        android:orientation="vertical"
        android:paddingLeft="@dimen/qb_px_5"
        android:paddingTop="@dimen/qb_px_10"
        android:paddingRight="@dimen/qb_px_5"
        android:paddingBottom="@dimen/qb_px_8">

        <ImageView
            android:id="@+id/img_homemenu_item_frgmthome"
            android:layout_width="@dimen/qb_px_32"
            android:layout_height="@dimen/qb_px_32"
            android:src="@drawable/ic_launcher_background" />

        <TextView
            android:id="@+id/tv_homemenu_item_frgmthome"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/qb_px_8"
            android:gravity="center"
            android:text="设备连接配置"
            android:textColor="@color/balck_0"
            android:textSize="@dimen/fs_px_14" />

        <TextView
            android:id="@+id/tv_keyid_item_frgmthome"
            android:layout_width="0dp"
            android:layout_height="0dp" />
    </LinearLayout>

</LinearLayout>